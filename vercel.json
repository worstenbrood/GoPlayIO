{
    "version": 2,
    "builds": [
        {
            "src": "addon.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "addon.js": {
          "maxDuration": 60
        },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "addon.js",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ],
    "installCommand": "pnpm install"
}
